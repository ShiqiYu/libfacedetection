train_net: "./yufacedetectnet.train.prototxt"

base_lr: 0.001
lr_policy: "poly"
power: 2

display: 10
max_iter: 200000
momentum: 0.9
weight_decay: 0.0005
snapshot: 5000
snapshot_prefix: "./models/yufacedetectnet-open-v2"
solver_mode: GPU
debug_info: false
snapshot_after_train: true
average_loss: 790
type: "SGD"
store_blobs_in_old_format: true
